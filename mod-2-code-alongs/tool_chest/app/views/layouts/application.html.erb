<!DOCTYPE html>
<html>
<head>
  <title>ToolChest</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <strong>Newest tool:</strong> <%= most_recent_tool.name %><br/>
  <p>
    <strong>Current Sessions Tool Summary:</strong> <%= current_tool_summary %>
  </p>
  <% flash.each do |type, message| %>
    <%= content_tag :div, message, class: "flash_#{type}" %>
  <% end %>
<%= yield %>

</body>
</html>
